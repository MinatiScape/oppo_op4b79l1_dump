<filter-conf>
    <version>20200904</version>
    <isOpen>1</isOpen>
    <filter-name>sys_high_temp_protect</filter-name>
    <HighTemperatureProtectSwitch>true</HighTemperatureProtectSwitch>
    <HighTemperatureShutdownSwitch>true</HighTemperatureShutdownSwitch>
    <HighTemperatureFirstStepSwitch>true</HighTemperatureFirstStepSwitch>
    <CountBatteryChangedForFosUpdateTime>10</CountBatteryChangedForFosUpdateTime>
    <HighTemperatureShutdownUpdateTime>15</HighTemperatureShutdownUpdateTime>
    <HighTemperatureProtectFirstStepIn>490</HighTemperatureProtectFirstStepIn>
    <HighTemperatureProtectFirstStepOut>470</HighTemperatureProtectFirstStepOut>
    <HighTemperatureProtectThresholdIn>510</HighTemperatureProtectThresholdIn>
    <HighTemperatureProtectThresholdOut>490</HighTemperatureProtectThresholdOut>
    <HighTemperatureProtectShutDown>750</HighTemperatureProtectShutDown>
    <MediumTemperatureProtectThreshold>10000</MediumTemperatureProtectThreshold>
    <HighTemperatureDisableFlashSwitch>true</HighTemperatureDisableFlashSwitch>
    <HighTemperatureDisableFlashLimit>420</HighTemperatureDisableFlashLimit>
    <HighTemperatureDisableFlashChargeLimit>470</HighTemperatureDisableFlashChargeLimit>
    <HighTemperatureDisableFlashLimitEnvHigh>490</HighTemperatureDisableFlashLimitEnvHigh>
    <HighTemperatureDisableFlashChargeLimitEnvHigh>440</HighTemperatureDisableFlashChargeLimitEnvHigh>
    <HighTemperatureDisableFlashCountTime>30</HighTemperatureDisableFlashCountTime>
    <camera_temperature_limit>425</camera_temperature_limit>
</filter-conf>
